{
  "OllamaApiService.Settings": {
    "SocketAddress": "localhost:11434",
    "UseHttps": false,
    "RequestTimeout": "-00:00:00.0010000"
  },
  "SerializableStorageService.Settings": {
    "Directory": "AppData"
  },
  "ConversationViewModel.Settings": {
    "SubjectRequest": {
      "model": "llama3:8b",
      "prompt": "Create a subject line with the following prompt \"*$Prompt\" \n The subject line MUST be less than 8 words total.",
      "stream":  true
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}